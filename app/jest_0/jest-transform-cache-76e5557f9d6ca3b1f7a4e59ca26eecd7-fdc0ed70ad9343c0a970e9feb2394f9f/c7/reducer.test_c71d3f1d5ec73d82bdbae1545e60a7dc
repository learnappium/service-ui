'use strict';var _reducer = require('./reducer');
var _constants = require('./constants');

describe('lang reducer', function () {
  test('should return initial state', function () {
    expect((0, _reducer.langReducer)(undefined, {})).toBe(_constants.INITIAL_STATE);
  });

  test('should return old state on unknown action', function () {
    var oldState = { foo: 1 };
    expect((0, _reducer.langReducer)(oldState, { type: 'foo' })).toBe(oldState);
  });

  test('should handle CHANGE_LANG_ACTION', function () {
    var payload = 'ru';
    var newState = (0, _reducer.langReducer)(undefined, {
      type: _constants.CHANGE_LANG_ACTION,
      payload: payload });

    expect(newState).toEqual(payload);
  });
