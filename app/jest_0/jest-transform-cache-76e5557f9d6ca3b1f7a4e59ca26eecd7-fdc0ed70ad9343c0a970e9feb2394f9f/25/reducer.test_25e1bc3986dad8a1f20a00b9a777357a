'use strict';var _constants = require('./constants');
var _reducer = require('./reducer');

describe('modal reducer', function () {
  test('should return initial state', function () {
    expect((0, _reducer.modalReducer)(undefined, {})).toBe(_constants.INITIAL_STATE);
  });

  test('should return old state on unknown action', function () {
    var oldState = { foo: 1 };
    expect((0, _reducer.modalReducer)(oldState, { type: 'foo' })).toBe(oldState);
  });

  test('should handle SHOW_MODAL', function () {
    var payload = {
      activeModal: { id: 'test', data: { foo: 'bar' } } };

    var newState = (0, _reducer.modalReducer)(undefined, {
      type: _constants.SHOW_MODAL,
      payload: payload });

    expect(newState).toEqual(payload);
  });

  test('should handle HIDE_MODAL', function () {
    var oldState = {
      activeModal: { id: 'test', data: { foo: 'bar' } } };

    var newState = (0, _reducer.modalReducer)(oldState, {
      type: _constants.HIDE_MODAL });

    expect(newState).toEqual({
      activeModal: null });

  });
