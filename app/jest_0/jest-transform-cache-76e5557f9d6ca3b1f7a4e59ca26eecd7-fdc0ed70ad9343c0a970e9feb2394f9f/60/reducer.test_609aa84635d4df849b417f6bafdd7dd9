'use strict';var _reducer = require('./reducer');
var _constants = require('./constants');

describe('appInfo reducer', function () {
  test('should return initial state', function () {
    expect((0, _reducer.appInfoReducer)(undefined, {})).toEqual({});
  });

  test('should return old state on unknown action', function () {
    var oldState = { foo: 1 };
    expect((0, _reducer.appInfoReducer)(oldState, { type: 'foo' })).toBe(oldState);
  });

  test('should handle FETCH_INFO_SUCCESS', function () {
    var payload = {
      foo: 123 };

    var newState = (0, _reducer.appInfoReducer)(undefined, {
      type: _constants.FETCH_INFO_SUCCESS,
      payload: payload });

    expect(newState).toEqual(payload);
  });
